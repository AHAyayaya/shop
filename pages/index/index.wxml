<!--index.wxml-->
<!-- 1. navigator页面跳转-->
<navigator url="../test/test">跳转到测试页面</navigator>

<!-- 2.redirect跳转 navigator属性：open-type-->
<navigator open-type="redirect" url="../test/test">跳转到测试页面(redirect跳转)</navigator>

<!-- 跳转过程中传递数据 -->
<navigator url="/pages/test/test?name=Chz&age=22&height=1.75">传递数据</navigator>
<view>{{value}}</view>

<!-- 通过事件代码进行跳转 -->
<button bindtap="jump">跳转到测试页面</button>

