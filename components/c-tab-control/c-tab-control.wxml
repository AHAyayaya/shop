<!--components/c-tab-control/c-tab-control.wxml-->
<view class="tab-control">
  <block wx:for="{{titles}}" wx:key="index">
    <view class="tab-item {{itemIndex == index ? 'active': ''}}" bindtap="itemClick" data-index="{{index}}">
      <text class="itembb">{{item}}</text>
    </view>
  </block>
</view>
